		{
			"name": "List TIme Slots",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const requestTimeout = parseInt(pm.environment.get(\"maxRequestTimeout\"));",
							"let jsonData = pm.response.json();",
							"",
							"pm.test(\"Get Time Slots schema to be correct\", function() {",
							"    const result = tv4.validateResult(jsonData, slotsSchema)",
							"    pm.expect(result.valid).to.be.true;",
							"});",
							"",
							"pm.test(\"Get Time Slots body to be correct\", function() {",
							"    pm.response.to.have.status(200);",
							"    pm.response.to.be.withBody;",
							"    pm.response.to.be.json;",
							"    pm.expect(pm.response.responseTime).to.be.below(requestTimeout);",
							"    pm.expect(jsonData).to.be.an.instanceof(Object).and.to.have.property('mentor').and.to.be.an.instanceOf(Object);",
							"    pm.expect(jsonData).to.have.property('calendar').and.to.be.an.instanceOf(Array);",
							"});"
						],
						"type": "text/javascript"
					}
				},
